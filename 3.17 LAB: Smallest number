/*
Write a program whose inputs are three integers, and whose output is the smallest of the three values.

Ex: If the input is:

7 15 3
the output is:

3
*/




#include <iostream>
using namespace std;

int main() {
   int a;
   int b;
   int c;
   
   cin >> a;
   cin >> b;
   cin >> c;
   
   if ((a <= b) && (a <= c)){
    cout << a << endl;  
   }
   else if ((b <= a) && (b <= c)){
    cout << b << endl;  
   }
   else if ((c <= b) && (c <= a)){
    cout << c << endl;  
   }

   return 0;
}
