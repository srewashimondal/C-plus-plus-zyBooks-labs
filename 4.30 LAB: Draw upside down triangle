/*
Write a program that outputs a right triangle of asterisks given the height as input. Each line ends with a blank space.

Ex: If the input is:

3
the output is:

* * * 
* * 
* 
*/





#include <iostream>
using namespace std;

int main() {
   int height;
   int i;
   int j;
   
   cin >> height;
   
   for (i = height; i >= 1; i--) {
      for (j = 1; j <= i; j++) {
         cout << "* ";
      }
    cout << endl;
  }
   
   
   return 0;
}
